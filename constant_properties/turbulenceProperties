// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//
// Laminar flow — no turbulence model.
//
// At Re ≈ 50 (nu = 0.01 m²/s, U = 1 m/s, L = 0.5 m) the flow is
// well within the laminar regime.
//
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  laminar;

